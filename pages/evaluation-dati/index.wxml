<!-- 第四套 -->
<view class="cont" style="" wx:if="{{cont==4}}">
  <view class="rid_top">
    <view class="greey">{{cont}}</view>
  </view>
  <view class="title">
    <text></text> {{cont_txt}}
  </view>
  <view class="count_xunze">
    <view class="{{tid4=='A'?'active':''}}" data-id="1" data-a="A" bindtap="clickFn4">
      <text style="font-size32rpx;"></text> {{opA}}</view>
    <view class="{{tid4=='B'?'active':''}}" data-id="2" data-a="B" bindtap="clickFn4">
      <text  style="font-size32rpx;"></text> {{opB}}</view>
    <view class="{{tid4=='C'?'active':''}}" data-id="3" data-a="C" bindtap="clickFn4">
      <text  style="font-size32rpx;"></text> {{opC}}</view>
  </view>
</view>
<view class="bottt">
  <view class="top_page" bindtap="toppage">上一页</view>
  <view class="nums">{{nums}}</view>
  <view class="bot_page" bindtap="pagebot">下一页</view>
</view>