    <view class="{{tid3==3?'active':''}}" data-a="C" data-id="3" bindtap="clickFn3">
      <text>③</text> 比较不符合</view>
    <view class="{{tid3==4?'active':''}}" data-a="D" data-id="4" bindtap="clickFn3">
